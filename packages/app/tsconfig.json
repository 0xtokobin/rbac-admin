{
  "extends": "@vue/tsconfig/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "lib": [
      "esnext",
      "dom"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "#/*": [
        "./types/*"
      ]
    },
    "types": [
      "@dcloudio/types"
    ],
    "sourceMap": true
  },
  "exclude": [
    "node_modules",
    "build"
  ],
  "include": [
    "types/**/*",
    "src/**/*",
    "serve/**/*",
    "mock/**/*",
    "./package.json"
  ]
}
