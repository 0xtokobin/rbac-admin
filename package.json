{
  "name": "@wingscloud/ui",
  "version": "0.0.0",
  "private": true,
  "description": "基于 Vue3 + Element Plus + Vite ，客户端渲染的 SPA 中后台权限管理应用程序。",
  "author": "Kaivan Wong <kaivanwong@outlook.com>",
  "license": "Apache 2.0",
  "homepage": "https://github.com/wingscloud/ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wingscloud/ui.git"
  },
  "bugs": {
    "url": "https://github.com/wingscloud/ui/issues"
  },
  "scripts": {
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "serve": "vite --config ./serve/index.ts",
    "serve-dev": "pnpm serve --mode development",
    "serve-stag": "pnpm serve --mode staging",
    "serve-prod": "pnpm serve --mode production",
    "build": "run-p type-check vite build --config ./serve/index.ts",
    "build-dev": "pnpm build --mode development",
    "build-stag": "pnpm build --mode staging",
    "build-prod": "pnpm build --mode production",
    "preview": "run-p type-check vite build --config ./serve/index.ts",
    "preview-dev": "pnpm preview --mode development",
    "preview-stag": "pnpm preview --mode staging",
    "preview-prod": "pnpm preview --mode production",
    "test": "vitest --config ./test/index.ts",
    "test-dev": "pnpm test --mode development",
    "test-stag": "pnpm test --mode staging",
    "test-prod": "pnpm test --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@kaivanwong/utils": "^0.3.5",
    "@vueuse/core": "^9.13.0",
    "@vueuse/integrations": "^9.13.0",
    "axios": "^1.3.4",
    "element-plus": "^2.2.32",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.32",
    "qs": "^6.11.0",
    "sass": "^1.58.3",
    "vue": "^3.2.47",
    "vue-dompurify-html": "^4.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@kaivanwong/eslint-config": "^0.13.14",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.35.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.8.4",
    "unocss": "^0.50.2",
    "unplugin-auto-import": "^0.15.0",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.24.0",
    "unplugin-vue-define-options": "^1.2.3",
    "vite": "^4.1.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.29.2",
    "vue-tsc": "^1.2.0"
  }
}
