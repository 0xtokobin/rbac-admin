{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "types/**/*",
    "src/**/*",
    "serve/**/*",
    "test/**/*",
    "mock/**/*",
    "./package.json"
  ],
  "exclude": [
    "node_modules",
    "build"
  ],
  "compilerOptions": {
    "composite": true,
    "moduleResolution": "node",
    "module": "ESNext",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "#/*": [
        "./types/*"
      ]
    }
  },
  "resolveJsonModule": true
}
