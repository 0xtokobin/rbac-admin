{
  "name": "@wingscloud/front-end-admin",
  "version": "0.0.0",
  "description": "✈️ 基于 Vue + Uniapp + Vite ，支持中后台 RBAC 应用及移动 OA 应用的前端系统。【↥ ↥ ↥ 点击 Star 关注更新】",
  "author": "Kaivan Wong <kaivanwong@outlook.com>",
  "license": "Apache 2.0",
  "homepage": "https://github.com/wingscloud/front-end#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wingscloud/front-end.git"
  },
  "bugs": {
    "url": "https://github.com/wingscloud/front-end/issues"
  },
  "scripts": {
    "serve": "vite --config ./serve/index.ts",
    "serve:dev": "pnpm serve --mode development",
    "serve:stag": "pnpm serve --mode staging",
    "serve:prod": "pnpm serve --mode production",
    "build": "run-p vue-tsc --noEmit vite build --config ./serve/index.ts",
    "build:dev": "pnpm build --mode development",
    "build:stag": "pnpm build --mode staging",
    "build:prod": "pnpm build --mode production",
    "preview": "run-p vue-tsc --noEmit vite build --config ./serve/index.ts",
    "preview:dev": "pnpm preview --mode development",
    "preview:stag": "pnpm preview --mode staging",
    "preview:prod": "pnpm preview --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@kaivanwong/utils": "^0.3.8",
    "@vueuse/core": "^9.13.0",
    "@vueuse/integrations": "^9.13.0",
    "element-plus": "^2.3.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.33",
    "sass": "^1.59.3",
    "tsparticles": "^2.9.3",
    "tsparticles-demo-configs": "^2.9.3",
    "tsparticles-engine": "^2.9.3",
    "vue": "^3.2.47",
    "vue-dompurify-html": "^4.0.0",
    "vue-router": "^4.1.6",
    "vue3-particles": "^2.9.3"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@kaivanwong/eslint-config": "^0.13.14",
    "@types/node": "^18.15.3",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.36.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.5",
    "unocss": "^0.50.4",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-define-options": "^1.2.4",
    "vite": "^4.1.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-tsconfig-paths": "^4.0.7",
    "vue-tsc": "^1.2.0"
