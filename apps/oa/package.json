{
  "name": "@wingscloud/front-end-oa",
  "version": "0.0.0",
  "private": true,
  "description": "✈️ 基于 Vue + Uniapp + Vite ，支持中后台 RBAC 应用及移动 OA 应用的前端系统。【↥ ↥ ↥ 点击 Star 关注更新】",
  "author": "Kaivan Wong <kaivanwong@outlook.com>",
  "license": "Apache 2.0",
  "homepage": "https://github.com/wingscloud/front-end#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wingscloud/front-end.git"
  },
  "bugs": {
    "url": "https://github.com/wingscloud/front-end/issues"
  },
  "scripts": {
    "serve-app": "uni -p app --config ./serve/index.ts",
    "serve-app:dev": "pnpm serve-app --mode development",
    "serve-app:stag": "pnpm serve-app --mode staging",
    "serve-app:prod": "pnpm serve-app --mode production",
    "serve-weixin": "uni -p mp-weixin --config ./serve/index.ts",
    "serve-weixin:dev": "pnpm serve-weixin --mode development",
    "serve-weixin:stag": "pnpm serve-weixin --mode staging",
    "serve-weixin:prod": "pnpm serve-weixin --mode production",
    "build-app": "run-p vue-tsc --noEmit uni build -p app --config ./serve/index.ts",
    "build-app:dev": "pnpm serve-app --mode development",
    "build-app:stag": "pnpm serve-app --mode staging",
    "build-app:prod": "pnpm serve-app --mode production",
    "build-weixin": "run-p vue-tsc --noEmit uni build -p mp-weixin --config ./serve/index.ts",
    "build-weixin:dev": "pnpm serve-weixin --mode development",
    "build-weixin:stag": "pnpm serve-weixin --mode staging",
    "build-weixin:prod": "pnpm serve-weixin --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@dcloudio/uni-app": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-app-plus": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-components": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-h5": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-mp-alipay": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-mp-baidu": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-mp-kuaishou": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-mp-lark": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-mp-qq": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-mp-toutiao": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-mp-weixin": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-quickapp-webview": "3.0.0-alpha-3061620221230002",
    "@kaivanwong/utils": "^0.3.8",
    "@vueuse/core": "^9.13.0",
    "@vueuse/integrations": "^9.13.0",
    "pinia": "^2.0.33",
    "sass": "^1.59.3",
    "tsparticles": "^2.9.3",
    "tsparticles-demo-configs": "^2.9.3",
    "tsparticles-engine": "^2.9.3",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue3-particles": "^2.9.3"
  },
  "devDependencies": {
    "@dcloudio/types": "^3.2.11",
    "@dcloudio/uni-automator": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-cli-shared": "3.0.0-alpha-3061620221230002",
    "@dcloudio/uni-stacktracey": "3.0.0-alpha-3061620221230002",
    "@dcloudio/vite-plugin-uni": "3.0.0-alpha-3061620221230002",
    "@kaivanwong/eslint-config": "^0.13.14",
    "@types/node": "^18.15.3",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.36.0",
    "npm-run-all": "^4.1.5",
